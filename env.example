# Thales CSM Akeyless Vault Configuration
# Choose one of these API URLs:
# For Akeyless cloud: https://api.akeyless.io
# For on-premise CipherTrust Manager: https://your-cm-hostname/akeyless-api/v2/
AKEYLESS_API_URL=https://api.akeyless.io
AKEYLESS_ACCESS_ID=your_access_id_here
AKEYLESS_ACCESS_KEY=your_access_key_here

# SSL Verification (set to false for self-signed certificates)
# For on-premise deployments with self-signed certs, set this to false
AKEYLESS_VERIFY_SSL=true 